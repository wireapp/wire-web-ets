{
  "dependencies": {
    "@types/compression": "0.0.36",
    "@types/dotenv": "4.0.3",
    "@types/express": "4.11.1",
    "@types/helmet": "0.0.37",
    "@types/node": "10.1.4",
    "@wireapp/core": "2.9.1",
    "@wireapp/lru-cache": "2.1.14",
    "body-parser": "1.18.3",
    "compression": "1.7.2",
    "dotenv": "5.0.1",
    "express": "4.16.3",
    "express-validator": "5.2.0",
    "helmet": "3.12.1",
    "pure-uuid": "1.5.2"
  },
  "description": "E2E Test Service",
  "devDependencies": {
    "husky": "0.14.3",
    "lint-staged": "7.1.2",
    "prettier": "1.13.2",
    "rimraf": "2.6.2",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.13.0",
    "tslint-plugin-prettier": "1.3.0"
  },
  "files": [
    "dist"
  ],
  "license": "GPL-3.0",
  "lint-staged": {
    "*.ts": [
      "tslint -c tslint.json --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "dist/index.js",
  "name": "wire-web-e2e-test-service",
  "repository": "https://github.com/wireapp/wire-web-e2e-test-service",
  "scripts": {
    "build": "yarn build:node",
    "build:node": "tsc",
    "clear": "rimraf dist",
    "dist": "yarn clear && yarn build",
    "fix": "yarn fix:other && yarn fix:ts",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix",
    "lint": "yarn lint:other && yarn lint:ts",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "tslint --config tslint.json --project tsconfig.json \"**/*.ts\"",
    "precommit": "lint-staged",
    "prettier": "prettier \"**/*.{json,md}\"",
    "start": "node dist/index.js",
    "test": "yarn dist && exit 0"
  },
  "types": "dist/index.d.ts",
  "version": "0.0.1"
}
